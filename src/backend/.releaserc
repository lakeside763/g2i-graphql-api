{
  "plugins": [
    '@semantic-release/commit-analyzer',
    '@semantic-release/release-notes-generator',
    '@semantic-release/npm',
    '@semantic-release/github'
  ],
  "preset": "angular",
  "branches": [
    '+([0-9])?(.{+([0-9]),x}).x',
    'main',
    'develop',
    'next',
    'next-major', {name: 'beta', prerelease: true}, {name: 'alpha', prerelease: true}
  ],
}
